{
  "name": "@demo-shop/source",
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "cm": "cz",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "docker:dev:rebuild": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
    "docker:prod": "docker-compose up -d --build",
    "start": "POSTGRES_HOST=localhost npx nx run-many --parallel --target=serve --projects=frontend,backend",
    "start:backend": "POSTGRES_HOST=localhost npx nx serve backend",
    "openapi:generate": "openapi-generator generate -g typescript-angular -o libs/api -i http://localhost:3000/api/json -c open-api.yaml",
    "prepare": "husky",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name initial-state",
    "prisma:reseed": "npm run prisma:reset && npm run prisma:seed",
    "prisma:reset": "POSTGRES_HOST=localhost npx prisma db push --force-reset",
    "prisma:seed": "POSTGRES_HOST=localhost npx prisma db seed",
    "prisma:studio": "POSTGRES_HOST=localhost npx prisma studio",
    "release:major": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "test": "npx nx run-many --target=test --all --coverage"
  },
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
